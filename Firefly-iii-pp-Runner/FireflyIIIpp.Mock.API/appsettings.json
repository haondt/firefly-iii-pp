{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ThunderClientEditorSettings": {
    "Path": "/thunder/",
    "ClientFile": "thunderclient.json",
    "EnvironmentFile": "thunderEnvironment.json",
    "CollectionFile": "thunderCollection.json",
    "CollectionName": "Firefly-III-pp-tests",
    "KeepPostmanIds": false
  },
  "FakeFireflyIIIServiceSettings": {
    "HttpDelayInMilliseconds": 200,
    "PageSize": 50
  },
  "KeyValueStoreSettings": {
    "Collection": "keyValueStore",
    "AutocompleteMaxResults": 10,
    "CreateCollections": true,
    "Stores": {
      "storeA": {
        "Path": "store1",
        "DefaultValueValue": "{\n}",
        "CreatePaths": true
      },
      "storeB": {
        "Path": "store2",
        "DefaultValueValue": "{\n}",
        "CreatePaths": true
      },
      "storeC": {
        "Path": "store3",
        "DefaultValueValue": "{\n}",
        "CreatePaths": true
      }
    }
  }
}
